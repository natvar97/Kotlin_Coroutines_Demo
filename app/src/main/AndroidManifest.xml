<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kotlin_coroutines_demo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kotlin_Coroutines_Demo">
        <activity android:name=".learn.timeout.TimeOutActivity"></activity>
        <activity android:name=".learn.task.twotask.TwoLongRunningTaskActivity" />
        <activity android:name=".learn.room.RoomDatabaseActivity" />
        <activity android:name=".learn.retrofit.single.SingleNetworkCallsActivity" />
        <activity android:name=".learn.retrofit.series.SeriesNetworkCallsActivity" />
        <activity android:name=".learn.retrofit.parallel.ParallelNetworkCallsActivity" />
        <activity android:name=".learn.errorhandling.trycatch.TryCatchActivity" />
        <activity android:name=".learn.errorhandling.supervisor.IgnoreErrorAndContinueActivity" />
        <activity android:name=".learn.errorhandling.exceptionhandler.ExceptionHandlerActivity" />
        <activity android:name=".RecyclerViewActivity" />
        <activity android:name=".learn.task.onetask.LongRunningTaskActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>